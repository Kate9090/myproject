html
  head
    title KEremina #{mypro}
    link(rel="shortcut icon" href="/img/favicon.ico" type="image/x-icon")
    link(rel="icon" href="/img/favicon.ico" type="image/x-icon")
    link(rel="stylesheet", href="css/main.css")
    block scripts
      script(src='js/main.js')
  body
    include template/preloader.pug
    block content
      .parallax
        .parallax__layer.parallax__layer--back
          include template/othor.pug


        .parallax__layer.parallax__layer--base

          include template/menu.pug
          include template/legend.pug
          include template/skills.pug
          include template/education.pug
          include template/portfolio.pug


    include template/back.pug


    block foot
      include template/footer.pug

    script(src="https://unpkg.com/react@16/umd/react.development.js" crossorigin)
    script(src="https://unpkg.com/react-dom@16/umd/react-dom.development.js" crossorigin)